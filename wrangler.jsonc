{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "expense-app",
	// Set this to today's date
	"compatibility_date": "2026-02-24",
	"assets": {
		"not_found_handling": "single-page-application"
	},
	"main": "./worker/index.ts",
	"queues": {
		"producers": [
			{
				"queue": "ai-extraction",
				"binding": "AI_EXTRACTION_QUEUE"
			}
		],
		"consumers": [
			{
				"queue": "ai-extraction",
				"max_batch_size": 1,
				"max_batch_timeout": 5
			}
		]
	}
}
